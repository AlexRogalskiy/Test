{
  "name": "test_alex_rogalsky",
  "version": "1.1.0",
  "description": "Testing new features",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
	"report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "mocha src/index.test.js -w",
    "test:single": "istanbul cover -x *.test.js node_modules/mocha/bin/_mocha -- -R spec src/index.test.js",
    "publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexRogalskiy/Test.git"
  },
  "keywords": [
    "testing",
    "new",
    "features"
  ],
  "author": "Alexander R <alexander.rogalsky@yandex.ru> (https://linkedin.com/in/alexander-rogalsky-985a4828)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexRogalskiy/Test/issues"
  },
  "homepage": "https://github.com/AlexRogalskiy/Test#readme",
  "dependencies": {
    "jsonfile": "2.3.1",
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.8.2",
    "cz-conventional-changelog": "1.1.6",
    "ghooks": "1.2.3",
    "istanbul": "0.4.3",
    "mocha": "2.5.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  }
}
